{
    "name": "glacourt/timber-starter-theme",
    "description": "Wordpress starter theme for Bedrock boilerplate and timber library",
    "type": "wordpress-theme",
    "require": {
        "symfony/form": "^5.0.0",
        "symfony/translation": "^5.0.0",
        "wp-cli/wp-cli-bundle": "^2.1@dev",
        "timber/timber": "dev-master",
        "symfony/twig-bridge": "^5.2@dev",
        "symfony/yaml": "^5.2@dev",
        "symfony/error-handler": "^5.2@dev",
        "symfony/var-dumper": "^5.2@dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev",
        "wp-coding-standards/wpcs": "dev-master",
        "mayflower/php-codebrowser": "dev-master",
        "pdepend/pdepend": "^2.0@dev",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "dev-master",
        "phpunit/php-code-coverage": "^7.0",
        "phpunit/phpunit": "8.5.x-dev",
        "sebastian/phpcpd": "^4.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Lacourt",
            "email": "guillaume.lacourt@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    }
}
